{
    
      "manifest_version": 2,
      "name": "UltrawideVideo",
      "version": "1.0",
    
      "description": "Fix fullscreen ultrawide video playback.",
    
      "icons": {
        "48": "icons/border-48.png"
      },
    
      "content_scripts": [
        {
          "matches": ["*://*.youtube.com/watch?v=*", "*://*.netflix.com/watch/*"],
          "js": ["script.js", "jquery-3.2.1.js"]
        }
      ]
    
    }